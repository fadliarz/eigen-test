    "helmet": "^7.1.0",
    "inversify": "^6.0.2",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
